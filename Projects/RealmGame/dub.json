	"name": "realmgame",
	"targetPath": "./bin",
	"workingDirectory": "../../",
	"dependencies": {
		"realm": "~main"
	},
	"extraDependencyFiles": [
		"./Assets/Shaders/*.shader",
		"./Assets/Shaders/*.glsl",
		"../../Assets/Shaders/*.shader"
	],
	"configurations": [
		{
			"name": "realm-executable",
			"targetType": "executable",
			"versions": [ "RealmExecutable" ]
		},
		{
			"name": "realm-dynamiclibrary",
			"targetType": "dynamicLibrary",
			"versions": [ "RealmDynamicLibrary" ]

		},
		{
			"name": "realm-hotreload",
			"targetType": "executable",
			"versions": [ "RealmHotReload" ]

		}

	]

}